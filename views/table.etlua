<div class="pure-g">
    <% for label in pairs(rows[1]) do %>
        <div class="pure-u-1 pure-u-md-1-3">        
            <p><%= label %></p>
        </div>
    <% end %>
    <% for i in pairs(rows) do %>
        <% for j, cell in pairs(rows[i]) do %>
            <div class="pure-u-1 pure-u-md-1-3">        
                <p><%= cell %></p>
            </div>
        <% end %>
    <% end %>
</div>
